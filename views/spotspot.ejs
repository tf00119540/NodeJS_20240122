<%- include('parts/html-head') %>
<%- include('parts/navbar') %>


<div class="container">
    <div class="row">
        <div class="col-6">
            <h2><%= title %></h2>
            <table class="table table-striped table-bordered">
                <thead>
                    <tr>
                        <th scope="col">編號</th>
                        <th scope="col">展覽名稱</th>
                        <th scope="col">地址</th>
                    </tr>
                </thead>
                <tbody>

                    <% for(let i of spotspot){ %>
                    <tr>
                        <td><%= i.id %></td>
                        <td><%= i.name %></td>
                        <td><%= i.location %></td>
                    </tr>
                    <% } %>
                    
                </tbody>
            </table>
        </div>
    </div>
</div>

<%- include('parts/scripts') %>
<%- include('parts/html-foot') %>